<%- include('header'); -%>
    <table class="product">
        <thead>
            <tr>
                <th>Title</th>
                <th>Price</th>
                <th>Description</th>
                <th>Image</th>
                <th>Rating</th>
            </tr>
        </thead>
        <% data.forEach(product => { %>
            <tr>
                <td><%- product.title %></td>
                <td><%- product.price %>&dollar;</td>
                <td><%- product.description %></td>
                <td><img src="<%- product.image %>" alt="Not found"></td>
                <td><%- product.rating.rate %> (<%- product.rating.count %>)</td>
            </tr>
        <% }); %>
    </table>
<%- include('footer'); -%>